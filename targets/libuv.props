<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ProjectDirectory Condition="'$(PathToLibuv)' != ''">$(PathToLibuv)</ProjectDirectory>
    <BuildCommand>build-native$(ShellExtension)</BuildCommand>
    <BuildPackagesCommand>build-packages$(ShellExtension)</BuildPackagesCommand>
    <PackagesOutput>bin/packages/$(Configuration)/</PackagesOutput>
  </PropertyGroup>
</Project>