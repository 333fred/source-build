<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="Build">
    <Message Importance="High" Text="Building '$(RepositoryName)'" />
    <Exec Command="$(DotNetCommand) restore $(ApplicationInsightsWrapperDirectory)Microsoft.ApplicationInsights.csproj /p:ProjectDirectory=$(ProjectDirectory) /p:Configuration=$(Configuration)" />
    <Exec Command="$(DotNetCommand) build $(ApplicationInsightsWrapperDirectory)Microsoft.ApplicationInsights.csproj /p:ProjectDirectory=$(ProjectDirectory) /p:Configuration=$(Configuration)" />
    <Exec Command="$(DotNetCommand) pack $(ApplicationInsightsWrapperDirectory)Microsoft.ApplicationInsights.csproj /p:ProjectDirectory=$(ProjectDirectory) /p:Configuration=$(Configuration)" />
  </Target>
</Project>
