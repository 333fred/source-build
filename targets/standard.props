<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ProjectDirectory Condition="'$(PathToStandard)' != ''">$(PathToStandard)</ProjectDirectory>
    <BuildCommand>build$(ShellExtension) -$(Configuration)</BuildCommand>
    <BuildPackagesCommand></BuildPackagesCommand>
    <PackagesOutput>bin/packages/$(Configuration)</PackagesOutput>
    <CleanCommand>clean$(ShellExtension)</CleanCommand>
  </PropertyGroup>
</Project>