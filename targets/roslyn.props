<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ProjectDirectory Condition="'$(PathToRoslyn)' != ''">$(PathToRoslyn)</ProjectDirectory>
    <BuildCommand>cibuild.cmd /$(Configuration) /skipTest /skipCommitPrinting</BuildCommand>
    <BuildCommand Condition="'$(OS)' != 'Windows_NT'">cibuild.sh --$(Configuration) --SkipTests --SkipCrossGen --SkipCommitPrinting</BuildCommand>
    <BuildPackagesCommand>Binaries/$(Configuration)/Exes/CsiCore/corerun Binaries/$(Configuration)/Exes/CsiCore/csi.exe src/NuGet/BuildNuGets.csx Binaries/$(Configuration) 42-localbuild Binaries/packages</BuildPackagesCommand>
    <PackagesOutput>Binaries/packages</PackagesOutput>
  </PropertyGroup>
</Project>