<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ProjectDirectory Condition="'$(PathToCoreclr)' != ''">$(PathToCoreclr)</ProjectDirectory>
    <BuildCommand>build$(ShellExtension)</BuildCommand>
    <BuildPackagesCommand>build-packages$(ShellExtension)</BuildPackagesCommand>
    <PackagesOutput>bin/Product/$(TargetOS).$(Platform).$(Configuration)/.nuget/pkg/</PackagesOutput>
    <CleanCommand>clean$(ShellExtension)</CleanCommand>
  </PropertyGroup>
</Project>